<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="styles/reset.css">
    <link rel="stylesheet" href="styles/style.css">
    <title>Product By Price</title>
</head>
<body>
    <header>
        <div>
            <a href="/">Home</a>
            <a href="/all">Maybelline Products</a>
            <a href="/mycards">Mycard</a>
        </div>
    </header>
    <section class="ProductSection">
    <% mArray.forEach(makeup =>{%> 
        <div>
            <img src="<%= makeup.image %> " alt="makeup">
            <h4> <%= makeup.name %> </h4>
            <p> <%= makeup.price %> </p>
            <p> <%= makeup.description %> </p>
        </div>
    <% }) %> 
</section>
    
</body>
</html>